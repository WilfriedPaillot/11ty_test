  "bugs": {
    "url": "https://github.com/WilfriedPaillot/11ty_test/issues"
  },
  "homepage": "https://github.com/WilfriedPaillot/11ty_test#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.3.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.43.3"
  },
  "devDependencies": {
    "bootstrap": "^5.1.3"
  }
}
